<ul id = "categoriess">
   <% Category.find_each do |c| %>
   <li><%= link_to c.title, :action => "show_categories", :id => c.id %></li>
   <% end %>
</ul>

<% if @feeds.blank? %>
<p> there are no feeds currently here </p>
<% else %>
<p> Blogs here </p>
<ul id = "feeds">
    <% @feeds.each do |c| %>
    <li>
    <%= link_to c.title, {:action => 'show', :id => c.id} -%>
    <b> <%= link_to 'Edit', {:action => 'edit', :id => c.id} %></b>
    <b> <%= link_to "Delete", {:action => 'delete', :id => c.id},
        :confirm => "Are you sure you want to delete this item?" %></b>
    </li>
    <% end %>
</ul>

<% end %>
<p><%= link_to "Add new Blogs", {:action => 'new' }%></p>